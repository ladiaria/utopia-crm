msgstr "No se pudo recuperar nada de %s."

#: util/validators.py:562
#, python-format
msgid ""
"The URL %(url)s returned the invalid Content-Type header '%(contenttype)s'."
msgstr "URL %(url)s devolvió: invalid Content-Type header '%(contenttype)s'."

#: util/validators.py:599
#, python-format
msgid ""
"Please close the unclosed %(tag)s tag from line %(line)s. (Line starts with "
"\"%(start)s\".)"
msgstr ""
"Por favor cierre las etiquetas %(tag)s sin cerrar de líneas %(line)s. (Línea "
"comienza con \"%(start)s\")."

#: util/validators.py:609
#, python-format
msgid ""
"Some text starting on line %(line)s is not allowed in that context. (Line "
"starts with \"%(start)s\".)"
msgstr ""
"Algún texto comenzando en la línea %(line)s no está permitido en ese "
"contexto. (Línea comienza con \"%(start)s\".)"

#: util/validators.py:620
#, python-format
msgid ""
"\"%(attr)s\" on line %(line)s is an invalid attribute. (Line starts with "
"\"%(start)s\".)"
msgstr ""
"\"%(attr)s\" en línea %(line)s es un atributo no válido. (Línea comienza con "
"\"%(start)s\".)"

#: util/validators.py:630
#, python-format
msgid ""
"\"<%(tag)s>\" on line %(line)s is an invalid tag. (Line starts with "
"\"%(start)s\".)"
msgstr ""
"\"<%(tag)s>\" en línea %(line)s es una etiqueta no válida. (Línea comienza "
"con \"%(start)s\".)"

#: util/validators.py:640
#, python-format
msgid ""
"A tag on line %(line)s is missing one or more required attributes. (Line "
"starts with \"%(start)s\".)"
msgstr ""
"Una etiqueta en la línea %(line)s está faltando o requiere más atributos. "
"(Línea comienza con \"%(start)s\".)"

#: util/validators.py:651
#, python-format
msgid ""
"The \"%(attr)s\" attribute on line %(line)s has an invalid value. (Line "
"starts with \"%(start)s\".)"
msgstr ""
"El atributo %(attr)s\" en la línea %(line)s tiene un valor no válido. (Línea "
"comienza con \"%(start)s\".)"

